<main class="col-md-8 ms-sm-auto col-lg-10 px-md-4">
    <div class="table-responsive">
        <div class="table-wrapper">
            <div class="table-title">
                <div class="row">
                    <div class="col-sm-5">
                        <h2>Reportes</h2>
                    </div>
                </div>
            </div>
            <table class="table table-striped table-hover" id="tableSales">
                <thead>
                    <tr>
                        <th>Fecha</th>
                        <th>Cliente</th>
                        <th>Producto</th>
                        <th>Cantidad</th>
                        <th>Precio</th>
                        <th>Total</th>
                    </tr>
                </thead>
                <tbody>
                    <% for (let i = 0; i < sales.length; i++) { %>
                        <tr>
                            <td><%= sales[i].date %></td>
                            <td><%= sales[i].client %></td>
                            <td><%= sales[i].product %></td>
                            <td><%= sales[i].quantity %></td>
                            <td><%= sales[i].price %></td>
                            <td><%= sales[i].total %></td>
                        </tr>
                    <% } %>
                </tbody>
            </table>
        </div>
    </div>
</main>