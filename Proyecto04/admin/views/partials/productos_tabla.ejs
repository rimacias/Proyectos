<main class="col-md-8 ms-sm-auto col-lg-10 px-md-4">
    <div class="table-responsive">
        <div class="table-wrapper">
            <div class="table-title">
                <div class="row">
                    <div class="col-sm-5">
                        <h2>Productos</h2>
                    </div>
                </div>
            </div>
            <table class="table table-striped table-hover">
                <thead>
                    <tr>
                        <th>Código</th>
                        <th>Nombre</th>
                        <th>Línea de Productos</th>
                        <th>Escala</th>
                        <th>Fabricante</th>
                        <th>Descripción</th>
                        <th>Stock</th>
                        <th>Precio de Venta</th>
                        <th>MSRP</th>
                    </tr>
                </thead>
                <tbody>  
                    <% aDatos.forEach((producto, idx) => { %>  
                      <tr>
                        <td><%= producto.productCode %></td>  
                        <td><%= producto.productName %></td>  
                        <td><%= producto.productLine %></td>  
                        <td><%= producto.productScale %></td>  
                        <td><%= producto.productVendor %></td>  
                        <td><%= producto.productDescription %></td>  
                        <td><%= producto.quantityInStock %></td>  
                        <td><%= producto.buyPrice %></td>  
                        <td><%= producto.MSRP %></td>  
                      </tr>  
                    <% }); %>  
                </tbody>  
              </table>
        </div>
    </div>  
</main>